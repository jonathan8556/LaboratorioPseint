Algoritmo Numeromayordedosnumeros
	Definir a, b como entero;
    
    Escribir "Ingrese el valor de a: ";
    Leer a;
    
    Escribir "Ingrese el valor de b: ";
    Leer b;
    
    Si a > b Entonces;
		Escribir "El mayor es a: ", a;
    Sino;
        Escribir "El mayor es b: ", b;
    FinSi;
FinAlgoritmo
