Algoritmo concienteyreciduo
	Definir A, B, cociente, residuo como entero;
    
    Escribir "Ingrese el valor de A: ";
    Leer A;
    
    Escribir "Ingrese el valor de B: ";
    Leer B;
    
    cociente = A / B;
    residuo = A MOD B;
    
    Escribir "El cociente es: ", cociente;
    Escribir "El residuo es: ", residuo;
FinAlgoritmo
